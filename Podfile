platform :ios, '10.0'

target 'TDD' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for TDD
  pod 'CollectionKit'
  target 'TDDTests' do
    inherit! :search_paths
    pod 'Nimble'
    pod 'Quick'
  end

end
